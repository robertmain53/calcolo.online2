@charset "utf-8";

a { color: rgb(0, 0, 255); text-decoration: none; }

a:visited { color: rgb(0, 0, 255); }

a:hover { color: rgb(228, 100, 28); text-decoration: underline; }

a.weblink { color: rgb(0, 0, 255); text-decoration: underline; }

a.weblink:visited { color: rgb(0, 0, 255); }

a.weblink:hover { color: rgb(228, 100, 28); }

a.popuplink { color: rgb(255, 0, 0); text-decoration: none; }

a.popuplink:visited { color: rgb(255, 0, 0); }

a.popuplink:hover { color: rgb(255, 0, 0); text-decoration: underline; }

a.filelink { color: rgb(4, 188, 20); text-decoration: none; }

a.filelink:visited { color: rgb(4, 188, 20); }

a.filelink:hover { color: rgb(4, 188, 20); text-decoration: underline; }

a.inline-toggle { color: green; text-decoration: none; font-weight: bold; f=
ont-family: "Times New Roman", serif; }

a.inline-toggle:visited { color: green; }

a.inline-toggle:hover { text-decoration: underline; }

.fsmall { font-size: 10px; font-family: Verdana, Helvetica, sans-serif; tex=
t-align: center; margin: 10px 0px 0px; }

.crumbs { font-size: 9pt; margin-bottom: 3px; margin-top: 0px; color: rgb(0=
, 0, 0); }

.crumbs a { text-decoration: underline; color: rgb(0, 0, 0); }

.crumbs a:visited { text-decoration: underline; color: rgb(0, 0, 0); }

.crumbs a:hover { color: rgb(228, 100, 28); }

@media screen {
  #idcontent { width: 100%; position: relative; padding: 0px !important; }
  #innerdiv { padding: 10px 5px 5px 10px !important; }
  .topichead { padding: 5px; }
  .navlinks { font-size: 10pt; }
  .navlinks a { text-decoration: none; color: blue; }
  .navlinks a:visited { text-decoration: none; color: blue; }
  .navlinks a:hover { text-decoration: underline; color: blue; }
  html.nonscroll { overflow: hidden; }
  body.nonscroll { overflow: hidden; height: 100%; }
  div.nonscroll { overflow: auto scroll; }
}
